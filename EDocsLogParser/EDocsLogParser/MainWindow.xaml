<Window x:Class="EDocsLog.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="eDOCS DM Server Log Parser" Height="140" Width="531">
    <Grid>
        <Button x:Name="btnStart" Content="Start" HorizontalAlignment="Left" Margin="90,10,0,0" VerticalAlignment="Top" Width="75" Click="btnStart_Click" IsEnabled="False"/>
        <ProgressBar x:Name="progressMain" HorizontalAlignment="Left" Height="13" Margin="10,79,0,0" VerticalAlignment="Top" Width="497"/>
        <Button x:Name="btnFolder" Content="_Directory..." HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="btnFolder_Click"/>
        <TextBlock x:Name="lblFolder" HorizontalAlignment="Left" Margin="10,37,0,0" VerticalAlignment="Top" Width="497"/>
        <TextBlock x:Name="lblStatus" HorizontalAlignment="Left" Margin="10,58,0,0" VerticalAlignment="Top" Width="497"/>
    </Grid>
</Window>
